{
  "name": "@ycdirectory/wallet-sdk",
  "version": "0.1.0",
  "description": "Modern wallet connection SDK with React components and multi-wallet support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react --jsx preserve",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react --jsx preserve",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "git clean -xdf .turbo node_modules out dist lib storybook-static",
    "prebuild": "rimraf dist",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "bump": "npm version ${VERSION_LEVEL:-patch} && git push --follow-tags",
    "release": "npm run bump && npm run build && npm publish",
    "release:minor": "VERSION_LEVEL=minor npm run release",
    "release:major": "VERSION_LEVEL=major npm run release"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^7.6.13",
    "@storybook/addon-interactions": "^7.6.13",
    "@storybook/addon-links": "^7.6.13",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.13",
    "@storybook/manager-api": "^7.6.13",
    "@storybook/react": "^7.6.13",
    "@storybook/react-vite": "^7.6.13",
    "@storybook/test": "^7.6.13",
    "@storybook/theming": "^7.6.13",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.5.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^7.6.13",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.8",
    "ethers": "^6.15.0",
    "viem": "^2.21.35",
    "wagmi": "^2.16.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ycdirectory-ui.git"
  },
  "keywords": [
    "react",
    "wallet",
    "ethereum",
    "web3",
    "blockchain",
    "sdk",
    "components",
    "rainbowkit",
    "wagmi"
  ],
  "author": "YCDirectory"
}